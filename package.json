{
  "name": "uber-direct-sdk",
  "version": "1.1.0",
  "description": "SDK for the Uberâ€™s delivery APIs enable merchants to integrate their own apps & services with Uber Direct order delivery",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "new-version": "changeset && npx changeset version",
    "tsup": "tsup --dts --format cjs,esm --external axios,jssha,zod",
    "publish": "npm run lint && npm run tsup && changeset publish",
    "lint": "eslint --fix --ext .ts src"
  },
  "keywords": [
    "SDK",
    "uber",
    "delivery"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandreL/UberDirectSDK.git"
  },
  "website": "https://github.com/alexandreL/UberDirectSDK",
  "author": "Alexandre Loubeyres",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "@changesets/cli": "^2.26.1"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "jssha": "^3.3.0",
    "zod": "^3.21.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
